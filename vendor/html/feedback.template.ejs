    <section class="reviewsForm" id="reviewsForm">
        <div class="container reviewsForm__container">
            <h3 id="reviewsForm__header"><i class="fas fa-comment"></i> Хотите оставить отзыв?</h3>
            <form action="" class="reviewsForm__form" name="reviewsForm" id="reviewsForm__form" autocomplete>
                <div class="formSet">
                    <label class="formSet__label" for="name">*Имя:</label>
                    <input type="text" name="name" id="name" class="input" placeholder="Представьтесь, пожалуйста"
                        required>
                </div>
                <div class="formSet">
                    <label class="formSet__label" for="email">*email:</label>
                    <input type="email" name="email" id="email" class="input" placeholder="Введите свой email" required>
                </div>
                <div class="formSet">
                    <span class="formSet__label formSet__label--icons">Иконка:</span>
                    <div class="radio__wrapper">
                        <label class="radio__picture" for="male">
                            <input type="radio" name="gender" id="male" value="male" checked>
                            <img src="img/userm.png" />
                        </label>
                        <label class="radio__picture" for="female">
                            <input type="radio" name="gender" id="female" value="female">
                            <img src="img/userf.png" />
                        </label>
                        <label class="radio__picture" for="couple">
                            <input type="radio" name="gender" id="couple" value="other">
                            <img src="img/user2.png" />
                        </label>
                        <label class="radio__picture" for="family">
                            <input type="radio" name="gender" id="family" value="family">
                            <img src="img/user3.png" />
                        </label>
                    </div>
                </div>
                <div class="formSet">
                    <label class="formSet__label" for="message">Отзыв:</label>
                    <textarea name="message" id="message" class="input" rows="5"
                        placeholder="Напишите свой отзыв здесь"></textarea>
                </div>
                <div class="formSet formSet__btn--feedback">
                    <input type="submit" value="Оставить отзыв" class="input-button" id="feedbackInputButton">
                </div>
                <div class="thanks">
                    <div class="popup">
                        Спасибо за Ваш отзыв!
                        <div class="closePopup">
                            <i class="fa fa-times"></i>
                        </div>
                    </div>
                </div>
            </form>

        </div>
    </section>
